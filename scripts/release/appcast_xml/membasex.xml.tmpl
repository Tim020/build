<?xml version="1.0"?>

<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:sparkle="http://www.andymatuschak.org/xml-namespaces/sparkle">
	<channel>
		<title>Updates for Couchbase Server</title>
		<link>http://appcast.couchbase.com/membasex.xml</link>
		<description>Recent changes to Membase.</description>
		<language>en</language>
		<item>
			<title>Version ${version}</title>
			<sparkle:releaseNotesLink>https://packages.couchbase.com/release_notes/${version}.html</sparkle:releaseNotesLink>
			<!-- date -u +"%a, %d %b %Y %H:%M:%S GMT" -->
			<pubDate>${date}</pubDate>
			<enclosure url="${file}" sparkle:version="${version}" length="${length}" type="application/octet-stream"/>
		</item>
	</channel>
</rss>
