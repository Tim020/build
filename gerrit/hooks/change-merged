#!/bin/sh

# change-merged --change <change id> --change-url <change url>
#               --project <project name> --branch <branch>
#               --submitter <submitter> --commit <sha1>


touch $HOME/review_site/logs/hook.log
exec >>$HOME/review_site/logs/hook.log 2>&1

echo =============================
echo "Change Merged Hook"

while [ $# -gt 0 ]
do
    arg=$1
    shift

    case "$arg" in
	--change)
	    change=$1
	    ;;
	--change-url)
	    change_url=$1
	    ;;
	--change-owner)
	    change_owner=$1
	    ;;
	--project)
	    project=$1
	    ;;
	--branch)
	    branch=$1
	    ;;
	--submitter)
	    submitter=$1
	    ;;
	--commit)
	    commit=$1
	    ;;
	--topic)
	    topic=$1
            ;;
	--kind)
	    change_kind=$1
	    ;;
	--newrev)
	    newrev=$1
	    ;;
	*)
	    echo "Unexpected arg:  $arg (but going forward anyway)"
	    ;;
    esac
    shift
done

case $project in
